{
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "useUnknownInCatchVariables": false,
    "target": "esnext",
    "moduleResolution": "bundler",
    "noImplicitAny": false,
    "sourceMap": false,
    "removeComments": true,
    "noImplicitThis": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "allowJs": false,
    "incremental": true
  }
}
